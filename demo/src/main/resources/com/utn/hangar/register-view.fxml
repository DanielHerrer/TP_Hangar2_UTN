<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="272.0" prefWidth="328.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.utn.hangar.RegisterController">
   <children>
      <Label layoutX="100.0" layoutY="58.0" text="Complete con sus datos:" />
      <TextField fx:id="inputUser" layoutX="90.0" layoutY="94.0" promptText="Ingrese usuario.." />
      <PasswordField fx:id="inputPass" layoutX="90.0" layoutY="129.0" promptText="Ingrese contraseña.." />
      <PasswordField fx:id="inputPassConfirm" layoutX="90.0" layoutY="162.0" promptText="Confirmar contraseña.." />
      <Button fx:id="btnRegister" layoutX="127.0" layoutY="216.0" mnemonicParsing="false" onAction="#onClickBtnRegister" text="Registrarse" />
      <Button fx:id="btnVolver" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onClickBtnVolver" text="Volver" />
   </children>
</AnchorPane>
